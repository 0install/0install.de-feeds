<?xml version="1.0" encoding="utf-8"?>
<interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>PuTTY</name>
  <summary xml:lang="en">Telnet and SSH client</summary>
  <summary xml:lang="de">Telnet und SSH Klient</summary>
  <description xml:lang="en">PuTTY is a free implementation of Telnet and SSH for Windows and Unix platforms, along with an xterm terminal emulator.</description>
  <description xml:lang="de">PuTTY ist eine freie Implementierung von Telnet und SSH f√ºr Windows und Unix Plattformen kombiniert mit einem xterm Terminal Emulator.</description>
  <homepage>http://www.chiark.greenend.org.uk/~sgtatham/putty/</homepage>
  <icon href="http://0install.de/feed-icons/PuTTY.png" type="image/png" />
  <icon href="http://0install.de/feed-icons/PuTTY.ico" type="image/vnd.microsoft.icon" />
  <category>Network</category>
  <category>Utility</category>
  <feed-for interface="http://0install.de/feeds/PuTTY.xml" />
  <group if-0install-version="2.6.8.." arch="Windows-*" license="MIT/X Consortium License">
    <command name="run" path="PUTTY.EXE" />
    <command name="putty" path="PUTTY.EXE" />
    <command name="puttygen" path="PUTTYGEN.EXE" />
    <command name="pageant" path="PAGEANT.EXE" />
    <command name="plink" path="PLINK.EXE" />
    <command name="pscp" path="PSCP.EXE" />
    <command name="psftp" path="PSFTP.EXE" />
    <implementation version="{version}" released="{released}" stability="stable">
      <manifest-digest />
      <archive href="http://the.earth.li/~sgtatham/putty/{version}/x86/putty.zip" type="application/zip" />
    </implementation>
  </group>
</interface>
