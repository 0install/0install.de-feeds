      <verb args="/oi &quot;%1&quot;" name="open"/>
      <extension value=".iso"/>
    </file-type>
    <auto-play id="CDBurnerXP" prog-id="CDBurnerXPOpenAutoPlay" provider="CDBurnerXP">
      <description xml:lang="en">Burn a disc</description>
      <verb args="/od &quot;%1&quot;" name="open"/>
      <event name="HandleBluRayBurningOnArrival"/>
      <event name="HandleCDBurningOnArrival"/>
      <event name="HandleDVDBurningOnArrival"/>
