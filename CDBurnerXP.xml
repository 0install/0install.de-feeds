      <description xml:lang="en">CDBurnerXP Data Compilation</description>
      <icon href="http://0install.de/feed-icons/CDBurnerXP_data.ico" type="image/vnd.microsoft.icon" xmlns="http://zero-install.sourceforge.net/2004/injector/interface"/>
      <verb args="/od &quot;%1&quot;" name="open"/>
      <extension value=".dxp"/>
    </file-type>
    <file-type id="CDBurnerXPAudio">
      <description xml:lang="en">CDBurnerXP Audio Compilation</description>
      <icon href="http://0install.de/feed-icons/CDBurnerXP_audio.ico" type="image/vnd.microsoft.icon" xmlns="http://zero-install.sourceforge.net/2004/injector/interface"/>
