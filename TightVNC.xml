  <entry-point command="run" binary-name="vncviewer">
    <name xml:lang="en">TightVNC Viewer</name>
    <icon href="http://0install.de/feed-icons/TightVNC/Viewer.png" type="image/png" />
    <icon href="http://0install.de/feed-icons/TightVNC/Viewer.ico" type="image/vnd.microsoft.icon" />
  </entry-point>
  <entry-point command="server" binary-name="winvnc">
    <name xml:lang="en">TightVNC Server</name>
    <icon href="http://0install.de/feed-icons/TightVNC/Server.png" type="image/png" />
    <icon href="http://0install.de/feed-icons/TightVNC/Server.ico" type="image/vnd.microsoft.icon" />
  </entry-point>
  <capabilities xmlns="http://0install.de/schema/desktop-integration/capabilities">
    <file-type description="VNCviewer Config File" id="TightVNC.VncViewer.Config">
      <verb name="open" args="-config &quot;%1&quot;" />
      <extension value=".vnc" />
    </file-type>
  </capabilities>
