<?xml version="1.0" encoding="utf-8"?>
<interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd">
  <name>Gow</name>
  <summary xml:lang="en">the lightweight alternative to Cygwin</summary>
  <homepage>https://github.com/bmatzelle/gow</homepage>
  <icon href="http://0install.de/feed-icons/Gnu.ico" type="image/vnd.microsoft.icon" />
  <icon href="http://0install.de/feed-icons/Gnu.png" type="image/png" />
  <category>Utility</category>
  <needs-terminal />
  <feed-for interface="http://0install.de/feeds/Gow.xml" />
  <group arch="Windows-*">
    <requires interface="http://0install.de/feeds/PuTTY.xml" importance="recommended">
      <executable-in-path command="plink" name="ssh" />
      <executable-in-path command="pscp" name="scp" />
      <executable-in-path command="psftp" name="sftp" />
    </requires>
    <environment name="PATH" insert="bin" />
    <executable-in-path command="gow" name="gow"/>
    <executable-in-path command="clear" name="clear"/>
    <executable-in-path command="whereis" name="whereis"/>
