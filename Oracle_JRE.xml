<?xml version="1.0" ?>
<interface uri="http://0install.de/feeds/Oracle_JRE.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd">
  <name>Oracle JRE</name>
  <summary xml:lang="en">official Oracle Java Runtime Environment</summary>
  <feed-for interface="http://repo.roscidus.com/java/openjdk-jre"/>
  <group license="Oracle Binary Code License Agreement for the Java SE Platform Products and JavaFX">
    <command name="run" path="bin/java.exe"/>
    <command name="run-gui" path="bin/javaw.exe"/>
    <command name="javaws" path="bin/javaws.exe"/>
    <command name="java-rmi" path="bin/java-rmi.exe"/>
    <command name="javacpl" path="bin/javacpl.exe"/>
    <command name="policytool" path="bin/policytool.exe"/>
    <command name="servertool" path="bin/servertool.exe"/>
    <command name="keytool" path="bin/keytool.exe"/>
    <command name="pak200" path="bin/pak200.exe"/>
    <command name="unpak200" path="bin/unpak200.exe"/>
    <implementation arch="Windows-x86_64" id="sha1new=11faf0265fab2c3cf105e3a892f7d69527eba723" stability="stable" version="7.0.45">
      <manifest-digest sha1new="11faf0265fab2c3cf105e3a892f7d69527eba723" sha256="73400bb6fd15f37bc89dffb7227ce21bf0cb9e74e17b61485849cf0703937656" sha256new="ONAAXNX5CXZXXSE5763SE7HCDPYMXHTU4F5WCSCYJHHQOA4TOZLA"/>
