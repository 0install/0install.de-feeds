    </implementation>
    <implementation arch="Windows-i486" id="sha1new=cd226027e47a099a400a6d0d0c6d4139476976bb" stability="stable" version="7.0.45">
      <manifest-digest sha1new="cd226027e47a099a400a6d0d0c6d4139476976bb" sha256="e6ffa1bf681c2c25f8591c2f261e614ed3efad7573d791185bc92f1105b72164" sha256new="4372DP3IDQWCL6CZDQXSMHTBJ3J67LLVOPLZCGC3ZEXRCBNXEFSA"/>
      <archive extract="jre1.7.0_45" href="http://0install.de/files/hosted/jre-7u45-windows-i586.tar.gz" size="41492480" type="application/x-compressed-tar"/>
    </implementation>
    <implementation arch="Windows-x86_64" id="sha1new=eca63b237ec347e1e3df7a087c65ab2754bc6024" released="2014-10-14" stability="stable" version="8.0.25">
