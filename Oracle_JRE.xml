  <entry-point binary-name="java" command="run">
    <needs-terminal/>
    <name xml:lang="en">Java command-line runner</name>
  </entry-point>
  <entry-point command="run-gui">
    <name xml:lang="en">Java GUI runner</name>
  </entry-point>
  <entry-point command="javaws">
    <name xml:lang="en">Java Web Start</name>
  </entry-point>
  <entry-point command="java-rmi">
    <needs-terminal/>
    <name xml:lang="en">Java Remote Method Invocation</name>
  </entry-point>
  <entry-point command="javacpl">
    <name xml:lang="en">Java Control Panel</name>
  </entry-point>
  <entry-point binary-name="policytool" command="policytool">
    <name xml:lang="en">Policy Tool</name>
  </entry-point>
  <entry-point binary-name="servertool" command="servertool">
    <needs-terminal/>
    <name xml:lang="en">Java IDL Server Tool</name>
  </entry-point>
  <entry-point binary-name="keytool" command="keytool">
    <needs-terminal/>
    <name xml:lang="en">Key and Certificate Management Tool</name>
  </entry-point>
  <entry-point binary-name="pak200" command="pak200">
    <needs-terminal/>
  </entry-point>
  <entry-point binary-name="unpak200" command="unpak200">
    <needs-terminal/>
  </entry-point>
  <capabilities os="Windows" xmlns="http://0install.de/schema/desktop-integration/capabilities">
    <file-type id="jarfile">
      <description xml:lang="en">Executable Jar File</description>
      <verb args="-jar &quot;%1&quot; %*" command="run-gui" name="open"/>
      <extension value=".jar"/>
    </file-type>
    <file-type id="JNLPFile">
      <description xml:lang="en">JNLP File</description>
      <verb args="&quot;%1&quot;" command="javaws" name="Open">
        <description xml:lang="en">&amp;Launch</description>
      </verb>
      <extension mime-type="application/x-java-jnlp-file" value=".jnlp"/>
    </file-type>
  </capabilities>
</interface>
