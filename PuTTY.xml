<?xml version="1.0" ?>
<interface uri="http://0install.de/feeds/PuTTY.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://zero-install.sourceforge.net/2004/injector/interface http://0install.de/schema/injector/interface/interface.xsd http://0install.de/schema/desktop-integration/capabilities http://0install.de/schema/desktop-integration/capabilities/capabilities.xsd">
  <name>PuTTY (legacy feed)</name>
  <summary xml:lang="en">Telnet and SSH client</summary>
  <summary xml:lang="de">Telnet und SSH Klient</summary>
  <description xml:lang="en">PuTTY is a free implementation of Telnet and SSH for Windows and Unix platforms, along with an xterm terminal emulator.</description>
  <description xml:lang="de">PuTTY ist eine freie Implementierung von Telnet und SSH f√ºr Windows und Unix Plattformen kombiniert mit einem xterm Terminal Emulator.</description>
  <homepage>https://www.chiark.greenend.org.uk/~sgtatham/putty/</homepage>
  <icon href="https://raw.githubusercontent.com/0install/0install.de-feeds/master/PuTTY.png" type="image/png"/>
  <icon href="https://raw.githubusercontent.com/0install/0install.de-feeds/master/PuTTY.ico" type="image/vnd.microsoft.icon"/>
  <category>Network</category>
  <category>Utility</category>

  <feed src="http://repo.roscidus.com/utils/putty" />

  <entry-point binary-name="putty" command="run"/>
  <entry-point command="puttygen">
    <name xml:lang="en">PuTTY Key Generator</name>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/puttygen.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/puttygen.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="pageant">
    <name xml:lang="en">PuTTY authentication agent</name>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/pageant.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/pageant.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="plink">
    <needs-terminal/>
    <name xml:lang="en">PuTTY Plink</name>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/plink.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/plink.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="pscp">
    <needs-terminal/>
    <name xml:lang="en">PuTTY Secure Copy</name>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/pscp.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/pscp.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="psftp">
    <needs-terminal/>
    <name xml:lang="en">PuTTY SFTP</name>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/psftp.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/repo.roscidus.com/master/utils/putty/psftp.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
</interface>
