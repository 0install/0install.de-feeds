  <entry-point binary-name="AutoIt3" command="run"/>
  <entry-point binary-name="Au3Info" command="info">
    <name>AutoIt Window Information Tool</name>
    <summary>allows you to get information from a specified window that can be used to effectively automate it</summary>
  </entry-point>
  <entry-point binary-name="Au3Check" command="check">
    <needs-terminal/>
    <name>AutoIt3 Syntax checker</name>
    <summary>checks the syntax of AutoIt v3 scripts</summary>
  </entry-point>
  <entry-point binary-name="AutoIt3Help" command="help">
    <name>AutoIt Help</name>
    <summary>help file with AutoIt documentation</summary>
  </entry-point>
  <entry-point binary-name="Aut2exe" command="aut2exe">
    <name>Aut2Exe</name>
    <summary>take your .au3 script and compile it into a standalone executable</summary>
  </entry-point>
  <entry-point binary-name="SciTE" command="scite">
    <name>SciTE Lite</name>
    <summary>a Scintilla based Text Editor modified by Jos for AutoIt3</summary>
  </entry-point>

  <capabilities os="Windows" xmlns="http://0install.de/schema/desktop-integration/capabilities">
    <file-type id="AutoIt3Script">
      <extension perceived-type="text" value=".au3"/>
      <description>AutoIt v3 Script</description>
      <icon href="https://raw.githubusercontent.com/0install/0install.de-feeds/master/AutoIt.script.ico" type="image/vnd.microsoft.icon" xmlns="http://zero-install.sourceforge.net/2004/injector/interface"/>
      <verb args="&quot;%1&quot; %*" command="run" name="open">
        <description>Run Script</description>
      </verb>
      <verb args="/in &quot;%l&quot;" command="aut2exe" name="compile">
        <description>Compile Script</description>
      </verb>
      <verb args="&quot;%1&quot;" command="scite" name="edit">
        <description>Edit Script</description>
