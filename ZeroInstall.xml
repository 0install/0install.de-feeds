    <command name="run" path="0install.exe">
      <runner command="run" interface="http://repo.roscidus.com/dotnet/clr"/>
    </command>
    <command name="run-gui" path="0install-win.exe">
      <runner command="run-gui" interface="http://repo.roscidus.com/dotnet/clr"/>
    </command>
    <command name="central" path="ZeroInstall.exe">
      <runner command="run-gui" interface="http://repo.roscidus.com/dotnet/clr"/>
    </command>
    <command name="update" path="0install-win.exe">
      <arg>maintenance</arg>
      <arg>deploy</arg>
      <arg>--batch</arg>
    </command>

